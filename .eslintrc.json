{
  "env": {
      "browser": true,
      "node": true
  },
  "parser": "@babel/eslint-parser",
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2021,
      "ecmaFeatures": {
          "jsx": true
      },
      "modules": true,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "no-console": 2,
    "quotes": [2, "single"],
    "semi": [2, "never"]
  }
}