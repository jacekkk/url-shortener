    "react-dom": "^16.8.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "core-js": "^3.12.1",
    "css-loader": "^2.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
